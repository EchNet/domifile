{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["name"],
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1
      },
      "action": {
        "type": "string",
        "enum": ["none", "classify", "broadcast", "tabulate"]
      }
    },
    "additionalProperties": false
  }
}
